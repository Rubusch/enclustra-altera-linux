/dts-v1/;
#include "socfpga_arria10_mercury_aa1_common.dtsi"

/ {
	model = "Enclustra Mercury AA1 (Rev1)";

	chosen {
		stdout-path = "serial0:115200n8";
	};
};

&uart0 {
	status = "okay";
};

&qspi {
	status = "disabled";

	flash0: n25q00@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "n25q00";
		reg = <0>;
		spi-max-frequency = <50000000>;
		cdns,page-size = <256>;
		cdns,block-size = <16>;
		m25p,fast-read;
		cdns,read-delay = <6>;
		cdns,tshsl-ns = <200>;
		cdns,tsd2d-ns = <255>;
		cdns,tchsh-ns = <20>;
		cdns,tslch-ns = <20>;

		part0: qspi-boot@0 {
			label = "Flash 0 Raw Data";
			reg = <0x0 0x01340000>;
		};
		part1: qspi-rootfs@1320000 {
			label = "Flash 1 jffs2 Filesystem";
			reg = <0x01340000 0x2cc0000>;
		};
	};
};
